# Google Cloud Core Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=us-central1

# BigQuery Tool Configuration
# Format: project.dataset.table
BIGQUERY_TABLE_ID=your-project.your_dataset.your_table
# Location of the BigQuery dataset (e.g., US, EU, us-central1)
BIGQUERY_LOCATION=US

# Vertex AI Agent Engine Configuration
# Fully qualified resource name: projects/{project}/locations/{location}/reasoningEngines/{id}
# Leave blank for initial deployment; update after deployment.
AGENT_ENGINE_ID=

# Services Toggle
USE_AGENT_ENGINE_MEMORY=false
USE_AGENT_ENGINE_SESSION=false

# Model selection (e.g., gemini-1.5-flash, gemini-2.0-flash)
MODEL=gemini-2.0-flash

# Telemetry and Observability
GOOGLE_CLOUD_AGENT_ENGINE_ENABLE_TELEMETRY=true
OTEL_INSTRUMENTATION_GENAI_CAPTURE_MESSAGE_CONTENT=true

# Gemini Enterprise Configuration (Optional)
# Managed OAuth Auth ID (from Gemini Enterprise Tool Context)
GEMINI_ENTERPRISE_AUTH_ID=
